// vercel.json
{
    "builds": [
        {
            "src": "index.html", // Gantilah dengan file entry point Vite Anda
            "use": "@sveltejs/adapter-static" // Gantilah dengan adapter yang sesuai dengan proyek Andaa
        }
    ],
    "routes": [
        {
            "src": "/assets/portfolio/(.*)",
            "dest": "/src/assets/portfolio/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}